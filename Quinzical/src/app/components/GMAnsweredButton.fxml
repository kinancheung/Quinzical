<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.effect.InnerShadow?>

<Button mnemonicParsing="false" prefHeight="45.0" prefWidth="120.0" style="-fx-background-radius: 15; -fx-font-weight: bold; -fx-background-insets: 1;" text="button" textAlignment="CENTER" textFill="HOTPINK" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <effect>
      <InnerShadow blurType="GAUSSIAN" color="HOTPINK">
         <input>
            <DropShadow blurType="GAUSSIAN" color="MAGENTA" offsetX="4.0" offsetY="4.0" />
         </input>
      </InnerShadow>
   </effect>
</Button>
